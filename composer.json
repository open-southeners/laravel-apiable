{
    "name": "open-southeners/laravel-apiable",
    "description": "Convert your Laravel application into one with a full API support and with extra capabilities",
    "license": "MIT",
    "keywords": [
        "open-southeners", "apiable", "json-api", "laravel"
    ],
    "authors": [
        {
            "name": "Ruben Robles",
            "email": "d8vjork@outlook.com"
        }
    ],
    "require": {
        "php": "^8.0",
        "illuminate/support": "^9.0",
        "open-southeners/laravel-helpers": "^0.6.0"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3.0",
        "laravel/pint": "^0.2.1",
        "nunomaduro/larastan": "^1.0",
        "orchestra/testbench": "^7.0",
        "phpstan/phpstan": "^1.0",
        "phpunit/phpunit": "^9.0"
    },
    "autoload": {
        "psr-4": {
            "OpenSoutheners\\LaravelApiable\\": "src",
            "OpenSoutheners\\LaravelApiable\\Tests\\": "tests"
        }
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "OpenSoutheners\\LaravelApiable\\ServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
